{
  "name": "site-pasticceria-dolcemente",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --ignore-unknown --ignore-path .gitignore",
    "prettier:diff": "yarn prettier --check",
    "prettier:write": "yarn prettier --write",
    "prettier:diff:all": "yarn prettier:diff .",
    "prettier:write:all": "yarn prettier:write .",
    "test": "jest src",
    "test-watch": "yarn test --watch",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-rest": "rimraf src/apis/rest && orval",
    "generate-graphql": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "axios": "^0.25.0",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-operations": "^2.2.4",
    "@graphql-codegen/typescript-react-apollo": "^3.2.5",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@storybook/testing-react": "^1.2.3",
    "@swc/core": "^1.2.137",
    "@swc/jest": "^0.2.17",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.15",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-storybook": "^0.5.6",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "msw": "^0.36.8",
    "msw-storybook-addon": "^1.6.0",
    "orval": "^6.5.3",
    "postcss": "^8.4.6",
    "postcss-preset-env": "^7.3.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.5",
    "react-icons": "^4.3.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.18",
    "typescript": "4.5.5",
    "webpack": "^5.68.0"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,md,graphql,json,yml}": [
      "yarn prettier:write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}